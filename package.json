{
  "name": "time-object",
  "version": "0.0.0-semantic-release",
  "description": "Wrap an object&#39;s functions such that each one logs execution time",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./ --ignore-pattern node_modules/ --ignore-pattern lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "npm run lint && npm run compile && nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dustinspecker/time-object.git"
  },
  "keywords": [
    "time",
    "object",
    "debug"
  ],
  "author": {
    "name": "Dustin Specker",
    "email": "DustinSpecker@DustinSpecker.com",
    "url": "https://github.com/dustinspecker"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "dependencies": {
    "is-plain-obj": "^1.1.0"
  },
  "devDependencies": {
    "austin": "^0.1.1",
    "ava": "^0.15.0",
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "coveralls": "^2.11.6",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^3.9.0",
    "eslint-config-dustinspecker": "^1.2.0",
    "eslint-plugin-no-use-extend-native": "^0.3.1",
    "eslint-plugin-xo": "^0.4.0",
    "nyc": "^6.1.1",
    "semantic-release": "^4.3.5"
  },
  "ava": {
    "files": [
      "tests/test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  }
}
